{
	"name": "Bauhaus",
	"version": "1.1.2",
	"description":
		"Web application for the management of concepts, classifications and other statistical objects",
	"repository": {
		"type": "git",
		"url": "https://github.com/InseeFr/Bauhaus"
	},
	"author": "INSEE (http://www.insee.fr)",
	"contributor": [
		{
			"name": "Nicolas Laval",
			"email": "nicolas.laval@insee.fr"
		}
	],
	"license": "MIT",
	"homepage": "./",
	"devDependencies": {
		"@storybook/addon-knobs": "^3.3.15",
		"@storybook/react": "^3.3.15",
		"babel-eslint": "^7.2.3",
		"coveralls": "^3.0.0",
		"cross-env": "^5.0.1",
		"eslint": "^4.1.1",
		"eslint-config-react-app": "^2.0.1",
		"eslint-plugin-flowtype": "^2.34.1",
		"eslint-plugin-import": "^2.6.0",
		"eslint-plugin-jsx-a11y": "^5.1.1",
		"eslint-plugin-react": "^7.1.0",
		"node-sass-chokidar": "^1.3.0",
		"npm-run-all": "^4.1.3",
		"prettier": "^1.9.2"
	},
	"dependencies": {
		"babel-polyfill": "^6.23.0",
		"bootstrap": "3.3.7",
		"create-react-class": "^15.6.2",
		"dompurify": "^0.8.7",
		"draft-js": "^0.10.4",
		"draft-js-export-html": "^1.2.0",
		"draft-js-import-html": "^1.2.1",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"file-saver": "^1.3.3",
		"jwt-decode": "^2.2.0",
		"keycloak": "^1.2.0",
		"lodash.deburr": "^4.1.0",
		"lodash.difference": "^4.5.0",
		"moment": "^2.18.1",
		"moment-range": "^3.0.3",
		"prop-types": "^15.5.10",
		"react": "16.2.0",
		"react-16-bootstrap-date-picker": "^5.1.1",
		"react-bootstrap": "^0.31.5",
		"react-bootstrap-table": "^4.3.0",
		"react-bootstrap-tabs": "^1.0.2",
		"react-d3-tree": "^1.10.3",
		"react-dom": "16.2.0",
		"react-draft-wysiwyg": "^1.12.0",
		"react-loading": "^1.0.3",
		"react-modal": "^3.1.8",
		"react-redux": "^5.0.2",
		"react-router-dom": "^4.1.1",
		"react-scripts": "^1.1.1",
		"react-select": "^1.1.0",
		"react-sortable-tree": "^2.1.0",
		"react-test-renderer": "^16.2.0",
		"redux": "^3.6.0",
		"redux-logger": "^2.8.1",
		"redux-thunk": "^2.2.0"
	},
	"scripts": {
		"build-css": "node-sass-chokidar src/ -o src/",
		"watch-css": "npm run build-css --watch --recursive",
		"start-js":
			"cross-env NODE_PATH=src/ REACT_APP_VERSION=$npm_package_version REACT_APP_NAME=$npm_package_name react-scripts start",
		"start": "npm-run-all -p watch-css start-js",
		"start-https-js":
			"cross-env NODE_PATH=src/ HTTPS=true REACT_APP_VERSION=$npm_package_version REACT_APP_NAME=$npm_package_name react-scripts start",
		"start-https": "npm-run-all -p watch-css start-https-js",
		"build-js":
			"cross-env NODE_PATH=src/ REACT_APP_VERSION=$npm_package_version REACT_APP_NAME=$npm_package_name react-scripts build",
		"build": "npm run build-css && build-js",
		"build-insee-js":
			"cross-env NODE_PATH=src/ REACT_APP_VERSION=$npm_package_version REACT_APP_NAME=$npm_package_name REACT_APP_INSEE=true react-scripts build",
		"build-insee": "npm run build-css && build-insee-js",
		"test-js": "cross-env NODE_PATH=src/  react-scripts test --env=jsdom",
		"test": "npm-run-all -p watch-css test-js",
		"test:coverage": "yarn test --coverage -i",
		"start-storybook-js": "cross-env NODE_PATH=src/ start-storybook -p 9999",
		"start-storybook": "npm-run-all -p watch-css start-storybook-js",
		"build-storybook-js":
			"cross-env NODE_PATH=src/ build-storybook -o built-storybook/storybook",
		"build-storybook": "npm run build-css && build-storybook-js",
		"eject": "react-scripts eject",
		"format":
			"prettier --trailing-comma es5 --single-quote --write 'src/**/*.js'"
	}
}
