{
	"name": "bauhaus",
	"version": "0.0.0",
	"license": "MIT",
	"private": true,
	"workspaces": [
		"packages/*",
		"app"
	],
	"scripts": {
		"start": "yarn workspace Bauhaus run start",
		"build-insee": "yarn workspaces run build-insee",
		"test:coverage": "yarn workspaces run test:coverage",
		"build-storybook": "yarn workspaces run build-storybook",
		"pre-push": "yarn workspaces run test:coverage && yarn workspaces run lint"
	},
	"resolutions": {
		"@inseefr/wilco": "0.0.3"
	},
	"devDependencies": {
		"@storybook/addon-actions": "5.3.18",
		"@storybook/addon-knobs": "5.3.18",
		"@storybook/addon-links": "5.3.18",
		"@storybook/addons": "5.3.18",
		"@storybook/react": "5.3.18",
		"@testing-library/jest-dom": "^5.1.1",
		"@testing-library/react": "10.0.3",
		"@types/jest": "25.2.1",
		"babel-loader": "8.1.0",
		"eslint": "6.8.0",
		"eslint-config-prettier": "6.7.0",
		"eslint-config-react-app": "5.1.0",
		"eslint-plugin-import": "2.19.1",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-react": "7.17.0",
		"husky": "3.0.2",
		"jest": "24.9.0",
		"jest-localstorage-mock": "2.4.0",
		"lcov-result-merger": "3.1.0",
		"webpack": "4.42.0"
	},
	"dependencies": {
		"prop-types": "15.7.2",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-redux": "7.2.0",
		"react-router-dom": "^5.1.2",
		"redux": "4.0.5"
	},
	"husky": {
		"hooks": {
			"pre-push": "yarn pre-push"
		}
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true
	}
}
